class Primenumber 
{
	public static void main(String[] args) 
	{
		int num=149;
		boolean f=true;
		for(int i=2; i<num; i++)
		{
			if(num%i==0)
			{
				f=false;
			}
		}
		if(f==true)
		{
			System.out.println(num+"---> prime number");
		}
		else
		{
			System.out.println(num+"---> not prime number");
		}
	}
}
