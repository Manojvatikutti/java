class Area1
{
	void circle(int r)
	{
		final double pi = 3.14;
		double res = pi*r*r;
		System.out.println(res);
	}
	void triangle(int b,int h)
	{
		double res = 0.5*b*h;
		System.out.println(res);
	}
	void square(int s)
	{
		int res = s*s;
		System.out.println(res);
	}
	void rectangle(int l,int b)
	{
		int res = l*b;
		System.out.println(res);
	}
	void parallelogram(int l, int b)
	{		
		int res = l*b;
		System.out.println(res);
	}
	void trapezaid(int a,int b,int h)
	{
		double res = 0.5*(a+b)*h;
		System.out.println(res);
	}
	void sector(int r,int deg)
	{
		double res = 0.5*r*r*deg;
		System.out.println(res);
	}
	void ellipse(int a,int b)
	{
		double pi = 3.14;
		double res = pi*a*b;
		System.out.println(res);
	}
	public static void main(String [] args)
	{
		Area area = new Area();
		area.circle(5);
		area.circle(7);

		System.out.println("*************************");

		Area area1 = new Area();
		area1.triangle(5,6);
		area1.triangle(6,7);

		System.out.println("*************************");

		Area area2 = new Area();
		area2.square(5);
		area2.square(7);

		System.out.println("*************************");

		Area area3 = new Area();
		area3.rectangle(6,7);
		area3.rectangle(8,9);

		System.out.println("*************************");

		Area area4 = new Area();
		area4.parallelogram(3,4);
		area4.parallelogram(5,6);

		System.out.println("*************************");

		Area area5 = new Area();
		area5.trapezaid(3,4,5);
		area5.trapezaid(5,6,7);

		System.out.println("*************************");

		Area area6 = new Area();
		area6.sector(5,30);
		area6.sector(7,50);

		System.out.println("*************************");

		Area area7 = new Area();
		area7.ellipse(5,6);
		area7.ellipse(10,11);
	}
}