class Rprime
{
	static boolean isPrime(int num)
	{
		for (int i=2;i<=Math.sqrt(num) ;i++ )
		{
			if(num%i == 0)
			{
				return false;
			}
		}
		return true;
	}
	public static void main(String [] args)
	{
		
		boolean res = isPrime(3);
		if (res)
		{
			System.out.println(" is a prime number");
		}
		else
		{
			System.out.println(" is not a prime number");
		}
	}
}